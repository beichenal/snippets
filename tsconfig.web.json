{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/pages/env.d.ts",
    "src/renderer/pages/**/*",
    "src/renderer/pages/**/*.tsx",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": ["src/renderer/*"],
      "@pages/*": ["src/renderer/pages/*"],
      "@mainPage/*": ["src/renderer/pages/main/*"],
      "@mainPageSrc/*": ["src/renderer/pages/main/src/*"]
    }
  }
}
